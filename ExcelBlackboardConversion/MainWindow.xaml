<Window x:Class="ExcelBlackboardConversion.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExcelBlackboardConversion"
        mc:Ignorable="d"
        Title="MainWindow" Height="165" Width="428">
    <Grid>
        <Button x:Name="button" Content="HTML Report" Margin="0,0,10,10" Click="button_Click" HorizontalAlignment="Right" Width="105" Height="41" VerticalAlignment="Bottom"/>
        <TextBox x:Name="txtFolderName" Margin="61,14,120,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top"/>
        <Label Content="Folder:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button1" Content="Select" Margin="0,13,10,0" VerticalAlignment="Top" Click="button1_Click" HorizontalAlignment="Right" Width="105"/>
        <Button x:Name="button2" Content="MarkPairs" HorizontalAlignment="Left" Margin="10,98,0,0" VerticalAlignment="Top" Width="106" Click="button2_Click" Height="41"/>

    </Grid>
</Window>
